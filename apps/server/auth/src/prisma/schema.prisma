generator client {
  provider        = "prisma-client-js"
  output = "../../../../../node_modules/@hostinfo/auth-prisma"

}

datasource db {
  provider = "sqlite"
  url      = "file:./db/db.sqlite" 
}

// DB model
// being sent to DB
model User {
  id Int @id @default(autoincrement())
  username String @unique
  password String 
}